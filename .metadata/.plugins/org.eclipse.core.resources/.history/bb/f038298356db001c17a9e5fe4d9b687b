
class Day8
{
	
	int id;
	String name;
	
	// constructor to initialize integer and string
	Day8(int i, String n)
	{
		id = i;
		name = n;
	}
	
	Day8()
	{}
	// constructor to initialize another object
	
	Day8(Day8 d)
	{
		id = d.id;
		name = d.name;
		}
	
	void display()
	{
		System.out.println(id + "" +name);
		}
	
	public static void main(String[] args) {
	// creating objects and passing values
	
	Day8 d1 = new Day8(111,"Karan");
	Day8 d2 = new Day8();
	
	d2.id = d1.id;
    d2.name = d1.name;
	d1.display();
	d2.display();
	
		
	}
}