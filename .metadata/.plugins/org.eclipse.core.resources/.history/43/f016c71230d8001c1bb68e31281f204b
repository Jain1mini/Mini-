
public class Pattern1 {

	public static void main(String[] args) {
		
		 
	String p="Python";
	char[]a=p.toCharArray();
	for (int i = 1; i<a.length; i++) {
		for (int j = i; j <a.length; j++) {
			System.out.print(a[i]);
		}
		System.out.println();
	}
	  }
}





