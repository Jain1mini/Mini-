
public class Pattern1 {

	public static void main(String[] args) {
		
		 
		int size = 5;
	    for (int i = 0; i <size ; i++) {
	      // print spaces
	      for (int j = 0; j <=i; j++) {
	        System.out.print(j+1);
	      }
	      // print stars
	     
	      }
	      System.out.println();
	    }
	  }
	
	